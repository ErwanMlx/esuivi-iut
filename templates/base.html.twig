{# {% form_theme form 'bootstrap_3_layout.html.twig' %} #}
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{% block title %}IUT Paris 13{% endblock %}</title>
    {% block stylesheets %}
      <link rel="stylesheet" type="text/css" href="/build/css/bootstrap-custom.css">
      <link rel="stylesheet" type="text/css" href="/build/bootstrap/css/bootstrap.min.css">
      <link rel="stylesheet" type="text/css" href="/build/css/global.css">
    {% endblock %}
</head>
<body>

{% block menu %}
<div id="menu">
  <div class="row">
    <div class="col-xs-8 col-sm-10">
      <div class="col-sm-offset-3">
        <img class="center-block" src="https://iutv.univ-paris13.fr/wp-content/uploads/2015/11/IUT-villetaneuse.png" width="160" height="136" alt=""/>
      </div>

    </div>
    <div class="avatar col-xs-4 col-sm-2 text-center">
      <span class="glyphicon glyphicon-user" aria-hidden="true" style="font-size: 70px;"></span>
      <h5>{% block user %}NOM Pr√©nom{% endblock %}</h5>
    </div>
  </div>
  <div class="row">
    <ul class="navigation">
      <li class="active"><a href="#">Page 1</a></li>
      <li><a href="#">Page 2</a></li>
      <li><a href="#">Page Suivi</a></li>
      <li><a href="#">Page 3</a></li>
    </ul>
  </div>
</div>
{% endblock %}

{% block body %}{% endblock %}

{% block javascripts %}
  <script src="/build/js/jquery-3.2.1.min.js"></script>
  <script src="/build/bootstrap/js/bootstrap.min.js"></script>
{% endblock %}
</body>
</html>
