<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Apprenti" table="apprenti">
    <indexes>
      <index name="IDX_2CB7951CE77D22BA" columns="id_dossier_apprenti"/>
      <index name="IDX_2CB7951C81B56FBD" columns="id_responsable_iut"/>
    </indexes>
    <id name="idCompte" association-key="true"/>
    <field name="telephone" type="string" column="telephone" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codePostal" type="string" column="code_postal" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="idCompte" target-entity="Compte" fetch="LAZY">
      <join-columns>
        <join-column name="id_compte" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idDossierApprenti" target-entity="DossierApprenti" fetch="LAZY">
      <join-columns>
        <join-column name="id_dossier_apprenti" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idResponsableIut" target-entity="Compte" fetch="LAZY">
      <join-columns>
        <join-column name="id_responsable_iut" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
