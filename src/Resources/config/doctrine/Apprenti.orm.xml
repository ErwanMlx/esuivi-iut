<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Apprenti" table="apprenti">
    <indexes>
      <index name="IDX_2CB7951C81B56FBD" columns="id_responsable_iut"/>
      <index name="IDX_2CB7951CE77D22BA" columns="id_dossier_apprenti"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="apprenti_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nom" type="string" column="nom" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codePostal" type="string" column="code_postal" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreationCompte" type="date" column="date_creation_compte" nullable="false">
      <options>
        <option name="default">now</option>
      </options>
    </field>
    <many-to-one field="idResponsableIut" target-entity="ResponsableIut" fetch="LAZY">
      <join-columns>
        <join-column name="id_responsable_iut" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDossierApprenti" target-entity="DossierApprenti" fetch="LAZY">
      <join-columns>
        <join-column name="id_dossier_apprenti" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
