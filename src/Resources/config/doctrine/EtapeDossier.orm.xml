<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\EtapeDossier" table="etape_dossier">
    <indexes>
      <index name="IDX_662143ECE3D54947" columns="id_dossier"/>
      <index name="IDX_662143EC1E108449" columns="id_validateur"/>
      <index name="IDX_662143EC83D972FD" columns="id_type_etape"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="etape_dossier_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="dateDebut" type="date" column="date_debut" nullable="false">
      <options>
        <option name="default">now</option>
      </options>
    </field>
    <field name="dateValidation" type="date" column="date_validation" nullable="true"/>
    <many-to-one field="idDossier" target-entity="DossierApprenti" fetch="LAZY">
      <join-columns>
        <join-column name="id_dossier" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idValidateur" target-entity="Compte" fetch="LAZY">
      <join-columns>
        <join-column name="id_validateur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTypeEtape" target-entity="TypeEtape" fetch="LAZY">
      <join-columns>
        <join-column name="id_type_etape" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
