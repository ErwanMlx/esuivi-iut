<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\ResponsableIut" table="responsable_iut">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="responsable_iut_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nom" type="string" column="nom" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="acces" type="boolean" column="acces" nullable="false">
      <options>
        <option name="comment">Admin = True</option>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
