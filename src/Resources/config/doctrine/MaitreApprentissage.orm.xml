<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\MaitreApprentissage" table="maitre_apprentissage">
    <indexes>
      <index name="IDX_FC0B595DA8937AB7" columns="id_entreprise"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="maitre_apprentissage_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nom" type="string" column="nom" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fonction" type="string" column="fonction" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fax" type="string" column="fax" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idEntreprise" target-entity="Entreprise" fetch="LAZY">
      <join-columns>
        <join-column name="id_entreprise" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
