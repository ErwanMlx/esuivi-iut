<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\MaitreApprentissage" table="maitre_apprentissage">
    <indexes>
      <index name="IDX_FC0B595DA8937AB7" columns="id_entreprise"/>
    </indexes>
    <id name="idCompte" association-key="true"/>
    <field name="fonction" type="string" column="fonction" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fax" type="string" column="fax" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="idCompte" target-entity="Compte" fetch="LAZY">
      <join-columns>
        <join-column name="id_compte" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idEntreprise" target-entity="Entreprise" fetch="LAZY">
      <join-columns>
        <join-column name="id_entreprise" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
