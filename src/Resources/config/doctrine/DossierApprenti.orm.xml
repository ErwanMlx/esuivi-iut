<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\DossierApprenti" table="dossier_apprenti">
    <indexes>
      <index name="IDX_4F7776BA8937AB7" columns="id_entreprise"/>
      <index name="IDX_4F7776BF72552AC" columns="id_etape_actuelle"/>
      <index name="IDX_4F7776BF0FBA8F9" columns="id_maitre_apprentissage"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="dossier_apprenti_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="etat" type="string" column="etat" length="16" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">En cours</option>
      </options>
    </field>
    <field name="dateEmbauche" type="date" column="date_embauche" nullable="true"/>
    <field name="sujetPropose" type="string" column="sujet_propose" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionDuSujet" type="string" column="description_du_sujet" length="512" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="participationFinanciere" type="integer" column="participation_financiere" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idEntreprise" target-entity="Entreprise" fetch="LAZY">
      <join-columns>
        <join-column name="id_entreprise" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEtapeActuelle" target-entity="EtapeDossier" fetch="LAZY">
      <join-columns>
        <join-column name="id_etape_actuelle" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMaitreApprentissage" target-entity="Compte" fetch="LAZY">
      <join-columns>
        <join-column name="id_maitre_apprentissage" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
